	70 k2 = 3.7E-9 * exp((-57000 / 8.314) * ((1 / TK) - (1 / 298.15))) * (MOL("H+") ^ -0.18) * (MOL("O2") ^ 0.50) 
	80 k = k1 + k2
	# kinetic data extracted from 00hol/cru 86mcm/bar 94wil/rim
	90 rate = S * m * Mm * ((m/m0)^(2/3)) * k * (1 - SR("Pyrite"))
